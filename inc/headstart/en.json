            "post_content": "<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"60px\",\"left\":\"60px\"},\"padding\":{\"bottom\":\"0px\"}}}} -->\n<div class=\"wp-block-group alignfull\" style=\"padding-bottom:0;\"><!-- wp:group {\"align\":\"full\",\"layout\":{\"contentSize\":\"1070px\"}} -->\n<div class=\"wp-block-group alignfull\"><!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"style\":{\"spacing\":{\"padding\":{\"right\":\"80px\"}}}} -->\n<div class=\"wp-block-column\" style=\"padding-right:80px;\"><!-- wp:heading {\"style\":{\"typography\":{\"textTransform\":\"uppercase\",\"fontSize\":\"47px\",\"fontStyle\":\"normal\",\"fontWeight\":\"200\",\"letterSpacing\":\"6px\",\"lineHeight\":1.3}}} -->\n<h2 style=\"font-size:47px;font-style:normal;font-weight:200;line-height:1.3;text-transform:uppercase;letter-spacing:6px;\">THE EARTH, ONE SHOT AT A TIME.<\/h2>\n<!-- \/wp:heading --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"400px\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:400px;\"><!-- wp:paragraph {\"style\":{\"typography\":{\"fontStyle\":\"normal\",\"fontWeight\":\"300\"}}} -->\n<p style=\"font-style:normal;font-weight:300;\">Welcome to the photography portfolio website of a nature photographer, <strong>John Appleton<\/strong>.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:cover {\"url\":\"https:\/\/appletondemo.files.wordpress.com\/2022\/05\/main.jpeg\",\"id\":55,\"dimRatio\":40,\"focalPoint\":{\"x\":\"0.50\",\"y\":\"0.20\"},\"minHeight\":650,\"customGradient\":\"linear-gradient(-180deg,rgba(0,0,0,0) 67%,rgb(0,0,0) 100%)\",\"contentPosition\":\"bottom left\",\"isDark\":false,\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"left\":\"40px\",\"bottom\":\"30px\"}}}} -->\n<div class=\"wp-block-cover alignfull is-light has-custom-content-position is-position-bottom-left\" style=\"padding-bottom:30px;padding-left:40px;min-height:650px;\"><span aria-hidden=\"true\" class=\"wp-block-cover__background has-background-dim-40 has-background-dim wp-block-cover__gradient-background has-background-gradient\" style=\"background:linear-gradient(-180deg,rgba(0,0,0,0) 67%,rgb(0,0,0) 100%);\"><\/span><img class=\"wp-block-cover__image-background wp-image-55\" alt=\"\" src=\"https:\/\/appletondemo.files.wordpress.com\/2022\/05\/main.jpeg\" style=\"object-position:50% 20%;\" data-object-fit=\"cover\" data-object-position=\"50% 20%\" \/><div class=\"wp-block-cover__inner-container\"><!-- wp:paragraph {\"align\":\"center\",\"placeholder\":\"Write title\u2026\",\"style\":{\"color\":{\"text\":\"#ffffffcc\"}},\"fontSize\":\"small\"} -->\n<p class=\"has-text-align-center has-text-color has-small-font-size\" style=\"color:#ffffffcc;\">Rock formations, Argentina.<\/p>\n<!-- \/wp:paragraph --><\/div><\/div>\n<!-- \/wp:cover --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"margin\":{\"top\":\"0px\",\"bottom\":\"0px\"},\"blockGap\":{\"top\":\"40px\",\"left\":\"40px\"},\"padding\":{\"top\":\"100px\",\"bottom\":\"120px\"}}},\"layout\":{\"contentSize\":\"1070px\"}} -->\n<div class=\"wp-block-group alignfull\" style=\"margin-top:0;margin-bottom:0;padding-top:100px;padding-bottom:120px;\"><!-- wp:heading {\"level\":4,\"style\":{\"typography\":{\"fontSize\":\"36px\",\"fontStyle\":\"normal\",\"fontWeight\":\"200\",\"letterSpacing\":\"-1px\",\"lineHeight\":1.5}}} -->\n<h4 style=\"font-size:36px;font-style:normal;font-weight:200;line-height:1.5;letter-spacing:-1px;\">I focus on representing landscapes and scenes that are usually hard to reproduce, thus capturing those ethereal moments. The technological tools are the brushes that I use to illuminate the surreal elements of nature.<\/h4>\n<!-- \/wp:heading -->\n\n<!-- wp:social-links {\"iconColor\":\"foreground\",\"iconColorValue\":\"#444444\",\"showLabels\":true,\"size\":